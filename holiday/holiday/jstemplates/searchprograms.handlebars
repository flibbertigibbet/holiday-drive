{{#each programs}}<a class="list-group-item accordion-toggle" data-toggle="collapse" href="#collapse{{id}}" data-parent="#accordion"><h3 class="list-group-item-heading">{{desc}}</h3></a><div id="collapse{{id}}" class="panel-collapse collapse"><div class="panel-body"><div class="list-group"><p class="list-group-item-text lead">Program Website: <a target="_blank" data-original-title="{{desc}} website" rel="tooltip" href="{{site}}">{{desc}}</a><a href="javascript:void(0)" onclick='confirm_sponsor("{{id}}", "{{desc}}")' class="btn btn-success sponsor-program pull-right" data-original-title="sponsor {{desc}}" rel="tooltip">Sponsor <span class="glyphicon glyphicon-ok"></span></a><br />Families: {{family_count}}<br />Children: {{children_count}}<br />{{#if program_types}}Program Types: <ul class="lead">{{#each program_types}}<li>{{this}}</li>{{/each}}</ul>{{else}}Program Type: {{program_type}}<br />{{/if}}</p><div class="list-group-item collapse"><div class="btn-toolbar lead"><strong>Wish list</strong></div></div>{{#with wishlist}}{{#each this}}<div class="list-group-item"><h4 class="list-group-item-heading">{{category}} - {{name}}: {{quantity}}</h4></div>{{/each}}{{/with}}</div></div></div>{{/each}}
