{{#each general_items}}<a class="list-group-item accordion-toggle" data-toggle="collapse" href="#collapse{{id}}" data-parent="#accordion"><h3 class="list-group-item-heading">{{name}}</h3></a><div id="collapse{{id}}" class="panel-collapse collapse"><div class="panel-body"><div class="list-group"><p class="list-group-item-text lead">Item category: {{category}}<a href="javascript:void(0)" onclick="confirm_sponsor('{{id}}', '{{name}}')" class="btn btn-success sponsor-item pull-right" data-original-title="sponsor {{name}}" rel="tooltip">Sponsor <span class="glyphicon glyphicon-ok"></span></a><br />Item name: {{name}}<br />Number requested: {{quantity}}<br />{{notes}}</p></div></div></div>{{/each}}
